{
	"compilerOptions": {
		"target": "es2016",
		"lib": ["ES2021", "dom"],
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,

		"module": "Node16",
		"moduleResolution": "Node16",

		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,

		"strict": true,
		"skipLibCheck": true
	}
}
